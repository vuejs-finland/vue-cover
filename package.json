{
  "name": "vue-cover",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "yarn cypress run --headed",
    "test-open": "yarn cypress open",
    "test-ci": "yarn cypress run",
    "lint": "yarn eslint \"src/**/*.{js,vue}\" --max-warnings 0",
    "lint-fix": "yarn eslint \"src/**/*.{js,vue}\" --fix"
  },
  "dependencies": {
    "buefy": "^0.9.6",
    "cypress": "^6.8.0",
    "lodash": "^4.17.21",
    "vue": "^3.0.5",
    "vue-router": "4",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^7.22.0",
    "eslint-plugin-vue": "^7.8.0",
    "typescript": "^4.1.3",
    "vite": "^2.1.0",
    "vue-tsc": "^0.0.8"
  },
  "license": "MIT"
}
